/// Sent after receiving [SMSG_AUTH_CHALLENGE].
/// This message is never encrypted.
cmsg CMSG_AUTH_SESSION = 0x1ED {
    u32 client_build;
    u32 login_server_id;
    CString username;
    u32 login_server_type;
    u32 client_seed;
    u32 region_id;
    u32 battleground_id;
    u32 realm_id;
    /// Purpose and exact meaning of name unknown.
    /// TrinityCore has this name but never uses the variable afterwards.
    u64 dos_response;
    u8[20] client_proof;
    u8[-] addon_info  {
        compressed = "true";
    }
} {
    versions = "3.3.5";
}
