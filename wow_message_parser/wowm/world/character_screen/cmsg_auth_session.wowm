struct AddonInfo {
    CString addon_name; 
    u8 addon_has_signature;
    u32 addon_crc;
    u32 addon_extra_crc;
} {
    versions = "1 2";
}

/// Sent after receiving [SMSG_AUTH_CHALLENGE].
/// Followed by [SMSG_AUTH_RESPONSE].
/// This message is never encrypted.
cmsg CMSG_AUTH_SESSION = 0x1ED {
    u32 build;
    /// This is sent to the client in [CMD_REALM_LIST_Server].
    u32 server_id;
    CString username;
    u32 client_seed;
    u8[20] client_proof;
    AddonInfo[-] addon_info {
        compressed = "true";
    }
} {
    versions = "1 2";
}

